{
  "name": "hybrix-jslib",
  "version": "0.5.14",
  "description": "javascript client library to interface with hybrixd",
  "main": "hybrix-lib.nodejs.js",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "engines": {
    "node": ">=12.13.0 <13.0.0"
  },
  "scripts": {
    "test": "sh ./scripts/npm/test.sh",
    "setup": "sh ./scripts/npm/setup.sh 'public'",
    "setup:dev": "sh ./scripts/npm/setup.sh 'dev'",
    "compile": "sh ./scripts/npm/compile.sh",
    "diag": "sh scripts/npm/diag.sh",
    "dist": "sh ./scripts/npm/dist.sh",
    "build": "sh ./scripts/npm/compile.sh; sh ./scripts/npm/dist.sh",
    "check": "sh scripts/npm/check.sh",
    "lint": "sh ../common/scripts/lint/lint.sh interface",
    "clean": "rm -rf dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hybrix-io/hybrix-jslib.git"
  },
  "author": "hybrix",
  "license": "GPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "uglify-es": "^3.3.9",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0"
  },
  "bugs": {
    "url": "https://github.com/hybrix-io/hybrix-jslib/issues"
  },
  "homepage": "https://hybrix.io"
}